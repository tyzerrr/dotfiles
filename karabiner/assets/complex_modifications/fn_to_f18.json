{
  "title": "Map fn to F18",
  "rules": [
    {
      "description": "Change fn key to F18",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "fn",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "f18"
            }
          ]
        }
      ]
    }
  ]
}

